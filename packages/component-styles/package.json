        "@rollup/plugin-node-resolve": "^11.2.0",
        "@x3r5e/design-tokens": "0.15.0",
        "copyfiles": "^2.4.1",
        "eslint": "^7.12.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.2.6",
        "postcss-preset-env": "^6.7.0",
