        "typed-scss-modules": "^4.1.0",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "@babel/runtime-corejs3": "^7.12.5",
        "@popperjs/core": "^2.6.0",
        "@x3r5e/component-styles": "0.11.1",
        "@x3r5e/icons": "0.10.1",
        "classnames": "^2.2.6",
        "core-js": "^3.8.1",
        "react-popper": "^2.2.4",
        "regenerator-runtime": "^0.13.7"
    }
}
