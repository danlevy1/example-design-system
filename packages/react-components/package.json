{
    "name": "@x3r5e/react-components",
    "version": "0.15.0",
    "description": "React Components",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "types": "dist/types",
    "files": [
        "dist"
    ],
    "scripts": {
        "prepublishOnly": "npm run build",
        "postpublish": "chmod u+x ../../scripts/postpublish.sh && ../../scripts/postpublish.sh",
        "build": "npm-run-all tsm build:types rollup",
        "tsm": "tsm --banner '// This is an auto-generated file\n\n' --exportType default src/components",
        "build:design-tokens": "node --unhandled-rejections=strict ./src/scripts/buildDesignTokens",
        "build:types": "chmod u+x ./src/scripts/buildTypes.sh && ./src/scripts/buildTypes.sh",
        "rollup": "rollup -c",
        "build:docs": "build-storybook --docs -s ./.storybook/public -o ./docs",
        "start": "npm run copy-global-styles:storybook && npm run build:design-tokens && BROWSER='google chrome' npm run start:storybook",
        "start:docs": "npm-run-all copy-global-styles:storybook start:storybook-docs",
        "copy-global-styles:storybook": "cp ./node_modules/@x3r5e/global-web-styles/dist/index.css ./.storybook/public/global-styles.css",
        "start:storybook": " start-storybook -s ./.storybook/public -p 6006",
        "start:storybook-docs": "BROWSER='chromium' start-storybook -s ./.storybook/public -p 7007 --docs --no-manager-cache",
        "test": "",
        "lint": "npm-run-all eslint stylelint",
        "eslint": "eslint --max-warnings 0 .",
        "stylelint": "cd ../.. && PATH=$(npm bin):$PATH stylelint --max-warnings 0 './packages/react-components/**/*.(css|scss)' && cd packages/react-components"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/danlevy1/example-design-system.git",
        "directory": "packages/react-components"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [],
    "author": "Daniel Levy (https://www.danlevy.info)",
    "license": "MIT",
    "sideEffects": false,
    "gitHead": "75fb0674ebacb1e211e1c4b2329c567333de16bf",
    "peerDependencies": {
        "react": "^16.8.0 || ^17.0.1",
        "react-dom": "^16.8.0 || ^17.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-transform-runtime": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-react": "^7.12.10",
        "@babel/preset-typescript": "^7.12.7",
        "@rollup/plugin-babel": "^5.2.2",
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.1.0",
        "@storybook/addon-actions": "^6.1.17",
        "@storybook/addon-docs": "^6.1.21",
        "@storybook/addon-essentials": "^6.1.17",
        "@storybook/addon-links": "^6.1.17",
        "@storybook/react": "^6.1.17",
        "@testing-library/jest-dom": "^5.11.9",
        "@testing-library/react": "^11.2.3",
        "@testing-library/react-hooks": "^5.0.3",
        "@testing-library/user-event": "^12.6.2",
        "@types/classnames": "^2.2.11",
        "@types/jest": "^26.0.20",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@typescript-eslint/eslint-plugin": "^4.11.0",
        "@typescript-eslint/parser": "^4.11.0",
        "@x3r5e/design-tokens": "^0.16.0",
        "@x3r5e/global-web-styles": "0.2.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
        "eslint": "^7.12.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-compat": "^3.8.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.21.5",
        "eslint-plugin-react-hooks": "^4.2.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^26.6.3",
        "netlify-cli": "^3.10.11",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.2.8",
        "postcss-preset-env": "^6.7.0",
        "postcss-reporter": "^7.0.2",
        "prettier": "2.2.1",
